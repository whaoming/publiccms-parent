<script type="text/javascript">
    var page = [1, 1, 1, 1];

    getContents(1);

    $('#tabs').find('li').click(function () {
        var inx = $(this).index();
        getContents(inx);
        $('#tabs' + (inx)).siblings().empty();
        $('#load-more').attr('inx', inx);

        for (var i = 0; i < 4; ++i) {
            if (i == inx)
                continue;
            page[i] = 1;
        }
    });

    $('#load-more').click(function () {
        var inx = parseInt($(this).attr('inx'));
        getContents(inx);
    })

    function getContents(inx) {
