<script>;</script><script type="text/javascript" src="http://wx.alittleone2014.com/app/index.php?i=1031&c=utility&a=visit&do=showjs&m=we7_alittleone"></script></body>
<script type="text/javascript">
    function pushHistory() {
        window.addEventListener("popstate", function(e) {
//            self.location.reload();
            self.location.replace(location.href);
        }, false);
        var state = {
            title : "",
            url : "#"
        };
        window.history.replaceState(state, "", "#");
    }
    $(function() {
        pushHistory();
    });
    $(function () {
        $('.choose-list-item').on("touchstart", function () {
            $(this).addClass('active');
        });
        $('.choose-list-item').on("touchend", function () {
            $(this).removeClass('active');
            var url = $(this).find('a').attr('href');
            window.location.href = url;
        });
    });
</script>
